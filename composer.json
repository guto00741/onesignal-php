{
    "name": "mingalevme/onesignal-simple-php-client",
    "description": "Simple OneSignal PHP Client",
    "license": "MIT",
    "keywords": ["onesignal"],
    "authors": [
        {
            "name": "Mikhail Mingaev",
            "email": "mingalevme@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-json": "*",
        "psr/log": "^1.1|^2.0|^3.0",
        "psr/http-factory": "^1.0",
        "psr/http-client": "^1.0"
    },
    "suggest": {
        "ext-zlib": "*",
        "guzzlehttp/guzzle": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Mingalevme\\OneSignal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mingalevme\\Tests\\OneSignal\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.5",
        "vlucas/phpdotenv": "^5.5",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.30",
        "phpstan/phpstan": "^1.9"
    }
}
